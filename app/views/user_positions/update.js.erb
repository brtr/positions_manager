$("tr[data-id='<%= @record.id %>']").each(function(){
  $(this).find('.notes').html("<%=j link_to (@record.notes&.truncate(10) || '备注历史'), user_positions_notes_histories_path(origin_symbol: @record.origin_symbol, trade_type: @record.trade_type, source: @record.source) %>")
})
$("#spinner").addClass("hide");
$("#editModal").modal('hide');