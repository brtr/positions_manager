<div class="modal fade" id="rankingModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5>
          <%= symbol %>
          <%= link_to '历史投入', position_detail_path(origin_symbol: symbol, source: source, trade_type: 'buy'), target: '_blank' %>
        </h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <div class="mt-3">
          <canvas id="priceChart" style="height: 300px; width: 780px" data-data="<%= data.to_json %>" ></canvas>
        </div>
      </div>
    </div>
  </div>
</div>